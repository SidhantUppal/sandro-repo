  <data name="dsDetail.SelectCommand" xml:space="preserve">
    <value>SELECT DATEADD(DD, DATEDIFF(DD,0,[HOUR_UTC]), 0) AS [DATE_UTC], 
	   SUM(dbo.AmountChangedIso(RECHARGES_TOTAL_AMOUNT, OPE_AMOUNT_CUR_ISO_CODE)) AS [RECHARGES_AMOUNT],
	   SUM(dbo.AmountChangedIso(RECHARGEREFUNDS_TOTAL_AMOUNT, OPE_AMOUNT_CUR_ISO_CODE)) AS [RECHARGEREFUNDS_AMOUNT],
	   SUM(dbo.AmountChangedIso(COUPONS_TOTAL_AMOUNT, OPE_AMOUNT_CUR_ISO_CODE)) AS [COUPONS_AMOUNT],
	   SUM(dbo.AmountChangedIso(COUPONREFUNDS_TOTAL_AMOUNT, OPE_AMOUNT_CUR_ISO_CODE)) AS [COUPONREFUNDS_AMOUNT],
	   SUM(dbo.AmountChangedIso(SERVICES_TOTAL_AMOUNT, OPE_AMOUNT_CUR_ISO_CODE)) AS [SERVICES_AMOUNT],
	   0 AS [DEPOSITS_AMOUNT]
FROM DB_OPERATIONS_HOUR
GROUP BY DATEADD(DD, DATEDIFF(DD,0,[HOUR_UTC]), 0)

</value>
  </data>
