<% if (ModelData.DetailData != null) {
%>
<script id="detailTemplate<%: ModelId %>" type="text/kendo-tmpl">
    
    <%
        var resBundleDetail = ResourceBundle.GetInstance(ModelData.DetailData.Definition.ResourcesAssemblyName);
     %>

        <%: Html.Kendo().TabStrip()
            .Name("tabDetail" + ModelId + "_#=" + ModelData.Definition.GetPKField().Mapping + "#")
            .SelectedIndex(0)
            .Animation(animation => animation.Open(open => open.Fade(FadeDirection.In)))
            .Items(items =>
            {
                items.Add().Text(resBundleDetail.GetString("Maintenance", string.Format("Fields.{0}", ModelData.DetailData.Name))).Content(obj => 

                    Html.Kendo().Grid(ModelData.DetailData.Data.Cast<dynamic>())
                    .Name("gridDetail" + ModelId + "_#=" + ModelData.Definition.GetPKField().Mapping + "#")
                    .Columns(columns =>
                    {
                        Kendo.Mvc.UI.Fluent.GridBoundColumnBuilder<dynamic> columnBuilder;
                        foreach (MaintenanceFieldDataModel field in ModelData.DetailData.Fields.Where(f => f.Hidden == false))
                        {
                            if (field.Type != MaintenanceFieldDataType.Template)
                            {                        
                                columnBuilder = columns.Bound(field.Mapping)
                                                .HtmlAttributes(field.Definition.GetHtmlAttributes(ModelId, true));

                                columnBuilder = columnBuilder.Title(field.LocalizedName)
                                                            .Format(field.Definition.GetFieldFormat(true))
                                                            .Width(field.Width);
                                if (field.Definition.GetClientTemplate(ModelId) != "")
                                {
                                    columnBuilder = columnBuilder.ClientTemplate(field.Definition.GetClientTemplate(ModelId, true));
                                }
                                field.Definition.SetFilterable(columnBuilder, ModelId);
                            }
                            else
                            {
                                columns.Bound(field.Mapping).ClientTemplate(field.Definition.Template)
                                       .Filterable(false).Sortable(false).Title(field.LocalizedName).Width(field.Width);
                            }                    
                        }
                    })
                    .DataSource(dataSource => dataSource
                        .Ajax()
                        .PageSize(10)
                        .Model(m =>
                        {
                            foreach (MaintenanceFieldDataModel field in ModelData.DetailData.Fields)
                            {
                                m.Field(field.Mapping, field.Definition.GetFieldType());
                                if (field.FKMaintenance != null) m.Field(string.Format("{0}_FK", field.Mapping), typeof(string));
                                if (field.IsPK) m.Id(field.Mapping);
                            }
                        })
                        .Read(read => read.Action("Grid_Read", "Maintenance", new { plugin = "MaintenancePlugin", modelName = ModelData.DetailData.Name, gridInitialized = "true" }).Data("function () { return { detailId_" + ModelData.Definition.DetailField.Replace(".", "_") + ": #=" + ModelData.Definition.GetPKField().Mapping +"#}; }"))
                        //.Create(create => create.Action("Grid_Create", "Maintenance", new { plugin = "MaintenancePlugin", modelName = ModelData.DetailData.Name }))
                        //.Update(update => update.Action("Grid_Update", "Maintenance", new { plugin = "MaintenancePlugin", modelName = ModelData.DetailData.Name }))
                        //.Destroy(destroy => destroy.Action("Grid_Destroy", "Maintenance", new { plugin = "MaintenancePlugin", modelName = ModelData.DetailData.Name }))
                        .Events(events =>
                        {
                            events.Error("ipsGrid" + ModelId + ".OnError");
                            events.RequestStart("ipsGrid" + ModelId + ".OnRequestStart");
                            events.RequestEnd("ipsGrid" + ModelId + ".OnRequestEnd");
                        })
                        .ServerOperation(true)
                        .Sort(sort =>
                        {
                            foreach (var field in ModelData.DetailData.Fields.Where(field => field.SortOrder > 0).OrderBy(field => field.SortOrder))
                            {
                                if (field.OrderType == MaintenanceFieldOrderType.Ascending)
                                    sort.Add(field.Mapping).Ascending();
                                else
                                    sort.Add(field.Mapping).Descending();
                            }
                        })                
                    )
                    .Pageable()
                    .Sortable()
                    .Resizable(resize => resize.Columns(true))
                    .ColumnMenu()
                    .ToClientTemplate()    
    
                );    

            })
            .ToClientTemplate()
        %>
</script>
<%} %>

