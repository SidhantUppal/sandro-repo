<% 
    var ModelId = Model.ModelId;
    var ModelName = Model.MaintenanceData.Name;
    var ModelData = Model.MaintenanceData;

    var sAssemblyBasePath = (!string.IsNullOrEmpty(ModelData.Definition.ResourcesAssemblyName) ? RouteConfig.BasePath.Replace("/MaintenancePlugin/", "/" + ModelData.Definition.ResourcesAssemblyName + "/") : RouteConfig.BasePath);
    if (System.IO.File.Exists(Server.MapPath(sAssemblyBasePath + "Views/Maintenances/" + ModelName + ".ascx")))
    {
        Html.RenderPartial(sAssemblyBasePath + "Views/Maintenances/" + ModelName + ".ascx", Model);
    }    

%>

<script type="text/javascript">
    //set culture of the Kendo UI
    kendo.culture("<%: System.Threading.Thread.CurrentThread.CurrentCulture  %>");

    <%
        var resBundle = ResourceBundle.GetInstance("MaintenancePlugin");
        var resBundle2 = ResourceBundle.GetInstance(ModelData.Definition.ResourcesAssemblyName);
    %>
    var ipsGrid<%: ModelId %> = new IPSGrid("<%: ModelName %>", "<%: ModelId %>",
            {
                actions: {
                    comboBox_read: "<%= Url.Action("ComboBox_Read", "Maintenance")%>",
                    getProfiles: "<%= Url.Action("GetProfiles", "Maintenance")%>",
                    loadProfile: "<%= Url.Action("GetProfile", "Maintenance")%>",
                    addProfile: "<%= Url.Action("AddProfile", "Maintenance")%>",
                    updateProfile: "<%= Url.Action("UpdateProfile", "Maintenance")%>",
                    deleteProfile: "<%= Url.Action("DeleteProfile", "Maintenance")%>"                    
                },
                messages: {
                    name: "<%= resBundle2.GetString("Maintenance", string.Format("Fields.{0}", ModelName), ModelName) %>",
                    boolean: {
                        yes: "<%= resBundle.GetString("Maintenance", "Messages.Boolean.Yes") %>",
                        no: "<%= resBundle.GetString("Maintenance", "Messages.Boolean.No") %>"
                    },
                    error: {
                        exception: "<%= resBundle.GetString("Maintenance", "Messages.Error.Exception", "Error") %>",
                        communication:"<%= resBundle.GetString("Maintenance", "Messages.Error.Communication", "Communication error") %>",
                        loadingdata: "<%= resBundle.GetString("Maintenance", "Messages.Error.LoadingData", "Error loading data") %>"
                    },
                    add: {
                        button: "<%= resBundle.GetString("Maintenance", "Messages.Add.Button", "Add") %>",
                        title: "<%= resBundle.GetString("Maintenance", "Messages.Add.Title", "Add new register") %>"
                    },
                    edit: {
                        button: "<%= resBundle.GetString("Maintenance", "Messages.Edit.Button", "Edit") %>",
                        title: "<%= resBundle.GetString("Maintenance", "Messages.Edit.Title", "Edit register") %>"
                    },
                    delete: {
                        button: "<%= resBundle.GetString("Maintenance", "Messages.Delete.Button", "Delete") %>",
                        title: "<%= resBundle.GetString("Maintenance", "Messages.Delete.Title", "Delete register") %>",
                        confirm: "<%= resBundle.GetString("Maintenance", "Messages.Delete.Confirm", "Are you sure you want to delete current register?") %>"
                    },
                    profile: {
                        load: {
                            title: "<%= resBundle.GetString("Maintenance", "Messages.Profile.Load.Title", "Load profile") %>",
                            lostChanges: "<%= resBundle.GetString("Maintenance", "Messages.Profile.Load.LostChanges", "Profile changed. Do you want lost changes?") %>",
                            error: {
                                invalid: "<%= resBundle.GetString("Maintenance", "Messages.Profile.Load.Error.Invalid", "Invalid profile") %>"
                            }
                        },
                        add: {
                            title: "<%= resBundle.GetString("Maintenance", "Messages.Profile.Add.Title", "Load profile") %>",
                            error: {
                                invalidName: "<%= resBundle.GetString("Maintenance", "Messages.Profile.Add.Error.InvalidName", "Invalid profile name") %>"
                            }
                        },
                        update: {
                            title: "<%= resBundle.GetString("Maintenance", "Messages.Profile.Update.Title", "Update profile") %>"                            
                        },
                        delete: {
                            title: "<%= resBundle.GetString("Maintenance", "Messages.Profile.Delete.Title", "Delete profile") %>",
                            confirm: "<%= resBundle.GetString("Maintenance", "Messages.Profile.Delete.Confirm", "Are you sure you want to delete selected profile?") %>"

                        }
                    },
                    filter: {
                        operator: {
                            eq: "<%= resBundle.GetString("Maintenance", "Messages.Filter.Operator.IsEqualTo", "Is equal to") %>",
                            neq: "<%= resBundle.GetString("Maintenance", "Messages.Filter.Operator.IsNotEqualTo", "Is not equal to") %>",
                            lt: "<%= resBundle.GetString("Maintenance", "Messages.Filter.Operator.IsLessThan", "Is less than") %>",
                            lte: "<%= resBundle.GetString("Maintenance", "Messages.Filter.Operator.IsLessThanOrEqualTo", "Is less than or equal to") %>",
                            gt: "<%= resBundle.GetString("Maintenance", "Messages.Filter.Operator.IsGreaterThan", "Is greater than") %>",
                            gte: "<%= resBundle.GetString("Maintenance", "Messages.Filter.Operator.IsGreaterThanOrEqualTo", "Is greater than or equal to") %>",
                            startswith: "<%= resBundle.GetString("Maintenance", "Messages.Filter.Operator.StartsWith", "Starts with") %>",
                            endswith: "<%= resBundle.GetString("Maintenance", "Messages.Filter.Operator.EndsWith", "Ends with") %>",
                            contains: "<%= resBundle.GetString("Maintenance", "Messages.Filter.Operator.Contains", "Contains") %>",
                            doesnotcontain: "<%= resBundle.GetString("Maintenance", "Messages.Filter.Operator.DoesNotContain", "Does not contain") %>",
                            lastweek: "<%= resBundle.GetString("Maintenance", "Messages.Filter.Operator.LastWeek", "In last week") %>",
                            lastmonth: "<%= resBundle.GetString("Maintenance", "Messages.Filter.Operator.LastMonth", "In last month") %>",
                            lastyear: "<%= resBundle.GetString("Maintenance", "Messages.Filter.Operator.LastYear", "In last year") %>",
                            currentweek: "<%= resBundle.GetString("Maintenance", "Messages.Filter.Operator.CurrentWeek", "In current week") %>",
                            currentmonth: "<%= resBundle.GetString("Maintenance", "Messages.Filter.Operator.CurrentMonth", "In current month") %>",
                            currentyear: "<%= resBundle.GetString("Maintenance", "Messages.Filter.Operator.CurrentMonth", "In current year") %>",
                            currentday: "<%= resBundle.GetString("Maintenance", "Messages.Filter.Operator.CurrentDay", "In current day") %>"
                        },
                        logic: {
                            and: "<%= resBundle.GetString("Maintenance", "Messages.Filter.Logic.And", "And") %>",
                            or: "<%= resBundle.GetString("Maintenance", "Messages.Filter.Logic.Or", "Or") %>"
                        }
                    }
                },
                /*profiles: %= Model.ProfilesJson() %,*/
                profilesActive: <%= (Model.ProfilesActive ? "true" : "false") %>,
                currentProfile: <%= Model.DefaultProfileJson %>,
                fields: <%= ModelData.Definition.FieldsJson() %>,
                varname: "ipsGrid<%: ModelId %>",
                routeConfig: {
                    basePath: "<%= RouteConfig.BasePath %>",
                    relBasePath: "<%= RouteConfig.RelBasePath %>",
                    absoluteBasePath: "<%= RouteConfig.GetContentUrl(RouteConfig.BasePath) %>"
                },
                shortDateFormat: "<%= System.Threading.Thread.CurrentThread.CurrentCulture.DateTimeFormat.ShortDatePattern %>",
                shortTimeFormat: "<%= System.Threading.Thread.CurrentThread.CurrentCulture.DateTimeFormat.ShortTimePattern %>",
                shortDateTimeFormat: "<%= System.Threading.Thread.CurrentThread.CurrentCulture.DateTimeFormat.ShortDatePattern + " " + System.Threading.Thread.CurrentThread.CurrentCulture.DateTimeFormat.ShortTimePattern %>",
                installationFilter: "<%= ModelData.InsFilter %>",
                mode: "<%= (ModelData.ModeAdvanced ? "advanced" : "basic") %>"
            });

    <% if (ModelData.DefinitionLoaded) {
        foreach (MaintenanceFieldDataModel oField in ModelData.Fields) {
           if (oField.FKMaintenance != null) {%>
                eval(<%= oField.Definition.GetFilterableJS(ModelId) %>);
                //eval('ipsGrid<%: ModelId %>.FK_<%: oField.Mapping %> = <%= "123"/*oField.GetFKMaintenanceDataJson()*/ %>');
        <% }
       }
       }%>
    
</script>
<ul class="edit-form-errors"></ul>

<%    
    if (ModelData.DefinitionLoaded)
    {
        if (ModelData.Access != AccessLevel.None)
        {            
            Html.Kendo().Grid(ModelData.Data.Cast<dynamic>())
            .Name("grid" + ModelId)
            .Columns(columns =>
            {
                Kendo.Mvc.UI.Fluent.GridBoundColumnBuilder<dynamic> columnBuilder;
                foreach (MaintenanceFieldDataModel field in ModelData.Fields.Where(f => f.Hidden == false))
                {
                    if (field.Type != MaintenanceFieldDataType.Template)
                    {
                        //if (field.FKMaintenance == null || field.FKMaintenance.DescriptionFields.Count == 0) //field.Type != MaintenanceFieldDataType.DropDown && field.Type != MaintenanceFieldDataType.Combo
                        columnBuilder = columns.Bound(field.Mapping)
                                        .HtmlAttributes(field.Definition.GetHtmlAttributes(ModelId));
                        //else
                        //{
                        //    field.FKMaintenance.LoadData();    
                        //    columnBuilder = columns.ForeignKey(field.Mapping, field.FKMaintenance.Data, field.FKMaintenanceIdField, field.FKMaintenanceDescField);
                        //}

                        columnBuilder = columnBuilder.Title(field.LocalizedName)
                                                    .Format(field.Definition.GetFieldFormat(true))
                                                    .Width(field.Width);
                        if (field.Definition.GetClientTemplate(ModelId) != "")
                        {
                            columnBuilder = columnBuilder.ClientTemplate(field.Definition.GetClientTemplate(ModelId));
                        }
                        field.Definition.SetFilterable(columnBuilder, ModelId);
                    }
                    else
                    {
                        columns.Bound(field.Mapping).ClientTemplate(field.Definition.Template)
                               .Filterable(false).Sortable(false).Title(field.LocalizedName).Width(field.Width);
                    }                    
                }
                if (ModelData.Access >= AccessLevel.Write)
                {
                    columns.Bound("Access").ClientTemplate("# if (Access >= " + ((int) AccessLevel.Write).ToString() + ") { # <a class=\"k-button k-button-icontext k-grid-edit\" href=\"\\#\"><span class=\"k-icon k-edit\"></span>#= ipsGrid" + ModelId +".options.messages.edit.button #</a> # } #" +
                                                     "# if (Access >= " + ((int) AccessLevel.Admin).ToString() + ") { # <a class=\"k-button k-button-icontext k-grid-delete\" href=\"\\#\"><span class=\"k-icon k-delete\"></span>#= ipsGrid" + ModelId + ".options.messages.delete.button #</a> # } #").Filterable(false).Sortable(false).Title("").Width(160);
                    //columns.Command(command =>
                    //    {                            
                    //        command.Edit();
                    //        if (ModelData.Access >= AccessLevel.Admin) command.Destroy();                            
                    //    }).Width(160);
                }
            })
            .ToolBar(toolbar =>
            {
                if (Model.ShowToolbar) {
                    toolbar.Template(() =>
                    {
                        Html.RenderPartial(RouteConfig.BasePath + "Views/Shared/MaintenanceToolbar.ascx", Model);
                    });
                }
            }
            )
            .Editable(editable => editable.Mode(GridEditMode.PopUp).TemplateName(RouteConfig.GetRelativePath("Views/Maintenance/EditorTemplates/") + "MaintenanceForm").AdditionalViewData(new { maintenance = Model }).Enabled(ModelData.Access >= AccessLevel.Write).DisplayDeleteConfirmation(false))
                //.Groupable()
            .Pageable(pager => pager
                .Input(false)
                .Numeric(true)
                .Info(true)
                .PreviousNext(true)
                .Refresh(true)
                .PageSizes(true)
            )
            .Sortable()
            .Scrollable()
            //.Groupable()
            .Selectable(conf => conf.Mode(GridSelectionMode.Single).Enabled(true).Type(GridSelectionType.Row))
                //.Scrollable(s => s.Height("auto"))
                //.Filterable(filterable => filterable
                //    .Extra(true)
                //.Operators(operators => operators
                //    .ForString(str => str.Clear()
                //        .Contains("Contains")
                //        .StartsWith("Starts with")
                //        .IsEqualTo("Is equal to")
                //        .IsNotEqualTo("Is not equal to")
                //    )
                //)
                //)
            .DataSource(dataSource => dataSource
                .Ajax()
                .PageSize(10)
                .Model(m =>
                {
                    foreach (MaintenanceFieldDataModel field in ModelData.Fields)
                    {
                        m.Field(field.Mapping, field.Definition.GetFieldType());
                        if (field.FKMaintenance != null) m.Field(string.Format("{0}_FK", field.Mapping), typeof(string));
                        if (field.IsPK) m.Id(field.Mapping);
                    }
                })
                .Read(read => read.Action("Grid_Read", "Maintenance", new { plugin = "MaintenancePlugin", modelName = ModelName, gridInitialized = "false" }))
                .Create(create => create.Action("Grid_Create", "Maintenance", new { plugin = "MaintenancePlugin", modelName = ModelName }))
                .Update(update => update.Action("Grid_Update", "Maintenance", new { plugin = "MaintenancePlugin", modelName = ModelName }))
                .Destroy(destroy => destroy.Action("Grid_Destroy", "Maintenance", new { plugin = "MaintenancePlugin", modelName = ModelName }))
                .Events(events =>
                {
                    events.Error("ipsGrid" + ModelId + ".OnError");
                    events.RequestStart("ipsGrid" + ModelId + ".OnRequestStart");
                    events.RequestEnd("ipsGrid" + ModelId + ".OnRequestEnd");
                })
                .ServerOperation(true)
                .Sort(sort =>
                {
                    foreach (var field in ModelData.Fields.Where(field => field.SortOrder > 0).OrderBy(field => field.SortOrder))
                    {
                        if (field.OrderType == MaintenanceFieldOrderType.Ascending)
                            sort.Add(field.Mapping).Ascending();
                        else
                            sort.Add(field.Mapping).Descending();
                    }
                })                
            )            
            .Resizable(resize => resize.Columns(true))
            .Reorderable(reorder => reorder.Columns(true))
            .HtmlAttributes(new { style = "height: 50px;", @class = "grid" })
            .Events(ev =>
            {
                ev.DataBound("ipsGrid" + ModelId + ".OnDataBound");
                ev.ColumnHide("ipsGrid" + ModelId + ".OnColumnHide");
                ev.ColumnShow("ipsGrid" + ModelId + ".OnColumnShow");
                ev.ColumnResize("ipsGrid" + ModelId + ".OnColumnResize");
                ev.ColumnReorder("ipsGrid" + ModelId + ".OnColumnReorder");
                ev.FilterMenuInit("ipsGrid" + ModelId + ".OnFilterMenuInit");
                ev.Edit("ipsGrid" + ModelId + ".OnEdit");
                ev.Cancel("ipsGrid" + ModelId + ".OnCancel");
                ev.SaveChanges("ipsGrid" + ModelId + ".OnSaveChanges");
                ev.Save("ipsGrid" + ModelId + ".OnSave");
                ev.Change("ipsGrid" + ModelId + ".OnChange");
            })
            .ColumnMenu()
            .ClientDetailTemplateId((ModelData.DetailData != null?"detailTemplate" + ModelId:""))
            .Render();

            //gridBuilder.Render();       
            
        }
        else
        {%>
            <%= Html.Label("lblAccessDenied",  FormAuthMemberShip.HelperService.AccessDeniedMessage() ) %>
        <%}
    }
%>

